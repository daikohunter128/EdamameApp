<!DOCTYPE html>
<html>
  <head>
    <title><%= document_title %></title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    "visitor", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "visitor", "data-turbolinks-track" => true %>
  </head>

  <body>
    <div id="wrapper">
      <%= render 'visitor/shared/header' %>
      <div id="container">
        <%= yield %>
      </div>
      <%= render 'shared/footer' %>
    </div>
  </body>
</html>